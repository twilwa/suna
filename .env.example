# Frontend
NEXT_PUBLIC_URL=https://suna.yourdomain.com
FRONTEND_PORT=3000
NEXT_PUBLIC_SUPABASE_URL=https://your-supabase-project-ref.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-anon-key
NEXT_PUBLIC_GOOGLE_CLIENT_ID

# Backend
BACKEND_PORT=8000
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key

# Supabase DB
SUPABASE_DATABASE_URL=postgresql://user:pass@ <supabase-host >:5432/dbname

# Redis / DragonFly
DRAGONFLY_REDIS_URL=redis://:password@ <dragonfly-host >:6379
REDIS_SSL=true

# LiteLLM
LITELLM_PORT=4000
# If using local weights:
# MODEL_PATH=/models/my-model.bin

# LLM Provider Keys (if backend proxies requests)
OPENAI_API_KEY=sk-...
ANTHROPIC_API_KEY=sk-ant-...

# Additional Tools
# DAYTONA_API_KEY=...
# CLOUDFLARE_API_TOKEN=...
# TAVILY_API_KEY=
# DAYTONA_SERVER_URL=
# DAYTONA_TARGET=
# RAPID_API_KEY=
